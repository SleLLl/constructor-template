{
  "name": "template",
  "version": "0.0.1",
  "author": "Kolyago Sergei <slelllq@gmail.com>",
  "license": "ISC",
  "description": "A TypeScript project template",
  "private": true,
  "keywords": [
    "React.js",
    "typescript"
  ],
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "buildstage": "tsc && vite build --mode development",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn tsc && yarn lint:format && yarn lint:fix",
    "type-check": "tsc"
  },
  "dependencies": {
    "html-react-parser": "^1.4.1",
    "i18next": "^21.4.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.13.0",
    "react-query": "^3.31.0",
    "react-router-dom": "^6.0.0",
    "react-toastify": "^8.1.0",
    "styled-components": "^5.3.3",
    "use-context-selector": "^1.3.9"
  },
  "devDependencies": {
    "@types/google.maps": "^3.47.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "typescript": "^4.3.5",
    "vite": "^2.6.13",
    "vite-plugin-imagemin": "^0.4.6"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "pre-commit": "lint"
}
